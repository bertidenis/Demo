<UserControl x:Class="VatService.View.UserPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:VatService.View"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <StackPanel Orientation="Vertical">
            <TextBlock Margin="5" x:Name="VATCODE"/>
            <TextBlock Margin="5" x:Name="COUNTRYCODE"/>
            <TextBlock Margin="5" x:Name="ADDRESS"/>
            <TextBlock Margin="5" x:Name="DATARQ"/>
            <TextBlock Margin="5" x:Name="NAME"/>
        </StackPanel>
    </Grid>
</UserControl>
